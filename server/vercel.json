{
    "version": 2,
    "env": {
        "DATABASE_URL": "@schedule_task_database_url",
        "POSTGRES_URL_NON_POOLING": "@schedule_task_database_url_non_pooling"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}